				"gtk-d:sv": "~>3.10.0",
				"gtk-d:vte": "~>3.10.0"
			}
		},
		{
			"dependencies": {
				"gtk-d:gstreamer": "~>3.10.0",
				"gtk-d:gtkd": "~>3.10.0",
				"gtk-d:peas": "~>3.10.0",
				"gtk-d:sv": "~>3.10.0",
				"gtk-d:vte": "~>3.10.0",
