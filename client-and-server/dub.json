			"excludedSourceFiles": [
				"client/source/app.d",
				"server/source/app.d",
				"client/source/client_cov_app.d",
				"server/source/server_cov_app.d"
			],
			"dependencies": {
				"gtk-d:gstreamer": "~>3.10.0",
				"gtk-d:gtkd": "~>3.10.0",
